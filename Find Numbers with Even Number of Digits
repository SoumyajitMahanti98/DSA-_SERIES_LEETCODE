class Solution {
    public int findNumbers(int[] nums) {
          int counter = 0;
          int n = nums.length;

        for (int i=0;i<n;i++) {
            String strNumber = Integer.toString(nums[i]);
            if (strNumber.length() % 2 == 0) {
                counter++;
            }
        }
        return counter;
    }
}
